generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model transaction {
  id        Int     @id @default(autoincrement())
  send      String
  received  String
  valor     Int
  createdAt DateTime @default(now()) 
}


model user {
  name     String
  email    String  @unique
  password String? @db.VarChar(255)
  id       String  @id @default(uuid())
}

model valor {
  id    Int    @id @default(autoincrement())
  name  String
  user  String @unique
  valor Int    @default(2000)
}